ation, setNavigation] = useState(['home'])\n\n  return (\n    <>\n      {/* Digital Clock */}\n      <Clock />\n\n      {/* Separater */}\n      <hr />\n      {/* Navigation */}\n      <div>\n        {navigation.map((item) => <span key = {item} onClick = {() => {\n          const ret = []\n          for (let i of navigation) {\n            ret.push(i)\n            if (i === item) {\n              break\n            }\n          }\n          if (ret.length === 1) {\n            setNavigation(ret)\n          }\n        }}>{item + '/'} </span>)}\n      </div>\n      {/* Title */}\n      <h2>Tasks</h2>\n      {/* Content */}\n      {\n        navigation.length === 1\n          ?\n        <>\n          <h3>Category</h3>\n          <div>\n            {\n              Object.keys(catInitState).map((key) => (\n                <>\n                <h5 onClick = {() => {setNavigation(['home', 'category', key])}}>{key}</h5>\n                <div key = {key}>{JSON.stringify(catInitState[key])}</div>\n                </>\n              ))\n            }\n          </div>\n          <h3>Tasks</h3>\n          <div>\n            {\n              Object.keys(taskInitState).map((key) => (\n                <>\n                <h5 onClick = {() => {setNavigation(['home', 'tasks', key])}}>{key}</h5>\n                <div key = {key}>{JSON.stringify(taskInitState[key])}</div>\n                </>\n              ))\n            }\n          </div>\n        </>\n          :\n        <>\n          <h4>{navigation[2]}</h4>\n          <div>\n            {\n              navigation[1] === 'category'\n                ?\n              JSON.stringify(catInitState[navigation[2]])\n                :\n              <div>{\n                Object.keys(taskInitState[navigation[2]]).map((key) => (\n                  <>\n                    <strong>{key}</strong>\n                    <p>{JSON.stringify(taskInitState[navigation[2]][key])}</p>\n                  </>\n                ))\n                }</div>\n            }\n          </div>\n        </>\n      }\n      \n    </>\n  )\n}\n\nexport default PageSchedule",["44","45"],"/Users/horatio/Desktop/eunomia-web/src/pages/PageMain.jsx",["46","47","48","49"],"import React, { useState, useReducer } from 'react'\nimport styled from 'styled-components'\nimport AgendaItem from './AgendaItem'\nimport PageSchedule from './PageSchedule'\n\n\nconst initialState = [\n  // 随着排序的进行，key可能会变化。想象一个场景：我们把第一个项目删了，再添加一个项目，\n  // 则 key 会重复。所以在这里应当删掉。\n  {\n    key: '0',\n    type: 'spot',\n    isChecked: false,\n    isStashed: false,\n    time: ['11:30'],\n    content: 'Get up, shower, brush teeth',\n  },\n  {\n    key: '1',\n    type: 'span',\n    isChecked: false,\n    isStashed: false,\n    time: ['00:30', '16:00'],\n    content: 'Eunomia app development',\n  },\n]\n\nconst categoryInitState = {\n  // Long-term, routine like activities,\n  // like work, study, fitness, chore, etc\n  'work': {},\n  'fit': {},\n}\n\nconst okrInitState = {\n  // Goals to achieve\n  'app development': {\n    content: 'to develop a time management application for mac and windows'\n  },\n  'calculus tutorial': {\n    content: 'to create a calculus tutorial based on Stewart Calculus'\n  }\n}\n\nconst reducer = (state, action) => {\n  const ret = [...state]\n  switch(action.type) {\n    case 'check':\n      ret[action.value].isChecked = !ret[action.value].isChecked\n      return ret\n    case 'stash':\n      ret[action.value].isStashed = !ret[action.value].isStashed\n      return ret\n    case 'append':\n      const item = {\n        key: ret.length.toString(),\n        ...action.value\n      }\n      ret.push(item)\n      ret.sort((a, b) => a.time[0].localeCompare(b.time[0]))\n      return ret\n    case 'timeChange':\n      ret[action.value].time[action.extra[0]] = action.extra[1]\n      ret.sort((a, b) => a.time[0].localeCompare(b.time[0]))\n      return ret\n    default:\n      return ret\n  }\n}\n\n\nconst categoryReducer = (state, action) => state\nconst okrReducer = (state, action) => state\n\n\nconst StyledPageWrapper = styled.div`\n// outer: position, size, margin\n  flex: 1;\n\n// inner: arrangement\n  padding: 0 20px 0 20px;\n\n// specs: color, behavior\n  border-right: 1px solid black;\n`\n\nconst StyledAgendaHeader = styled.div`\n`\nconst StyledAgendaList = styled.div`\n`\nconst StyledAgendaInput = styled.div`\n  display: flex;\n  width: 400px;\n  margin-top: 15px;\n`\n\nconst TaskPageWrapper = styled.div`\n  // flex: 0.8;\n  height: 100%;\n  overflow-y: auto;\n  width: 230px;\n  border-right: 1px solid black;\n`\n\n\nconst parseInput = (s, dispatch) => {\n  const item = {\n    type: 'spot',\n    isChecked: false,\n    isStashed: false,\n    time: [],\n    content: '',\n  }\n  item.time.push(s.substring(0, 5))\n  item.content = s.substring(6)\n  // console.log(item)\n  dispatch({type: 'append', value: item})\n}\n\n\n\nconst PageTodolist = () => {\n\n  const [todoState, todoDispatch] = useReducer(reducer, initialState)\n  const [catState, catDispatch] = useReducer(categoryReducer, categoryInitState)\n  const [okrState, okrDispatch] = useReducer(okrReducer, okrInitState)\n\n  const [agendaInput, setAgendaInput] = useState('')\n\n  return (\n    <>\n\n    <StyledPageWrapper>\n\n      <StyledAgendaHeader>\n        <h2>Today</h2>\n      </StyledAgendaHeader>\n      \n\n      <StyledAgendaList>\n        {todoState.map((item, index) =>\n          <AgendaItem key = {item.key}\n            info = {item}\n            id = {index}\n            dispatch = {todoDispatch}\n          />\n        )}\n      </StyledAgendaList>\n\n      {/* Input */}\n      <StyledAgendaInput>\n        <input type = 'textarea' value = {agendaInput}\n          onChange = {(event) => setAgendaInput(event.target.value)}\n          onKeyPress = {(event) => {\n            if (event.key === 'Enter') {\n              parseInput(agendaInput, todoDispatch)\n              setAgendaInput('')\n            }\n          }}\n          style = {{flex: 1}}\n        />\n        <button onClick = {() => {\n          parseInput(agendaInput, todoDispatch)\n          setAgendaInput('')\n        }}>\n          Add\n        </button>\n      </StyledAgendaInput>\n\n    </StyledPageWrapper>\n\n\n    <TaskPageWrapper>\n      <PageSchedule />\n    </TaskPageWrapper>\n    </>\n  )\n}\n\n\nexport default PageTodolist","/Users/horatio/Desktop/eunomia-web/src/pages/AgendaItem.jsx",[],"/Users/horatio/Desktop/eunomia-web/src/Tabs.jsx",[],"/Users/horatio/Desktop/eunomia-web/src/Components/TaskList.jsx",[],["50","51"],"/Users/horatio/Desktop/eunomia-web/src/Components/TodoList.jsx",[],"/Users/horatio/Desktop/eunomia-web/src/reducer.jsx",[],"/Users/horatio/Desktop/eunomia-web/src/Components/TodoItem.jsx",[],{"ruleId":"52","severity":1,"message":"53","line":1,"column":27,"nodeType":"54","messageId":"55","endLine":1,"endColumn":37},{"ruleId":"52","severity":1,"message":"56","line":45,"column":10,"nodeType":"54","messageId":"55","endLine":45,"endColumn":14},{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","replacedBy":"60"},{"ruleId":"52","severity":1,"message":"61","line":125,"column":10,"nodeType":"54","messageId":"55","endLine":125,"endColumn":18},{"ruleId":"52","severity":1,"message":"62","line":125,"column":20,"nodeType":"54","messageId":"55","endLine":125,"endColumn":31},{"ruleId":"52","severity":1,"message":"63","line":126,"column":10,"nodeType":"54","messageId":"55","endLine":126,"endColumn":18},{"ruleId":"52","severity":1,"message":"64","line":126,"column":20,"nodeType":"54","messageId":"55","endLine":126,"endColumn":31},{"ruleId":"57","replacedBy":"65"},{"ruleId":"59","replacedBy":"66"},"no-unused-vars","'useReducer' is defined but never used.","Identifier","unusedVar","'time' is assigned a value but never used.","no-native-reassign",["67"],"no-negated-in-lhs",["68"],"'catState' is assigned a value but never used.","'catDispatch' is assigned a value but never used.","'okrState' is assigned a value but never used.","'okrDispat